<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
        title="About Project">
    <fx:Script><![CDATA[
        private function clickSendCommentHandler(event:MouseEvent):void {
            navigator.pushView(CommentView);
        }
        ]]></fx:Script>
    <s:Group width="100%" height="100%">
        <s:layout>
            <s:VerticalLayout  horizontalAlign="center" verticalAlign="middle"/>
        </s:layout>

        <s:Button label="Send comment" click="clickSendCommentHandler(event)"/>
    </s:Group>

</s:View>
